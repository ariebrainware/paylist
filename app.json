{
  "expo": {
    "name": "Paylist",
    "description": "Pay bill, set budget per month, and jot down some buy list easily with Paylist",
    "slug": "Paylist",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.ritalin.paylist"
    },
    "ios": {
      "supportsTablet": true
    }
  }
}